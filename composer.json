{
  "name": "php/scrap",
  "type": "project",
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "pecee/simple-router": "5.4.1.7",
    "twig/twig": "^3.0",
    "vlucas/phpdotenv": "^5.6"
  },
  "autoload": {
    "psr-4": {
      "App\\": "App/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
